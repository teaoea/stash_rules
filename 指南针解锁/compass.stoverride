name: 指南针定位解锁
desc: 指南针定位解锁
category: app解锁
http:
  mitm:
    - "buy.itunes.apple.com"
  script:
    - match: ^https?:\/\/buy\.itunes\.apple\.com\/verifyReceipt$
      name: compass_unlock
      type: response
      require-body: true
      timeout: 10
script-providers:
  compass_unlock:
    url: https://raw.githubusercontent.com/teaoea/stash_rules/refs/heads/main/指南针解锁/compass.js
    interval: 86400
